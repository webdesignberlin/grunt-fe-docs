{
  "name": "grunt-fe-docs",
  "description": "CSS Docs & Style Guide generator based on DSS and the grunt-css-docs plugin from roughcoder (https://github.com/roughcoder/grunt-css-docs). (Similar to JSDoc format) Template support Handlebars, Hogan, Jade, Mustache",
  "version": "1.2.0",
  "homepage": "https://github.com/Webastronaut/grunt-fe-docs",
  "author": "Philip Morkisch <pm@webastronaut.org> (https://github.com/Webastronaut)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Webastronaut/grunt-fe-docs"
  },
  "bugs": {
    "url": "https://github.com/Webastronaut/grunt-fe-docs/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Webastronaut/grunt-fe-docs/blob/master/LICENSE-MIT"
    }
  ],
  "main": "Gruntfile.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "scripts": {
    "test": "grunt test"
  },
  "dependencies": {
    "dss": "1.0.3",
    "marked": "~0.3.0",
    "handlebars": "~1.3.0",
    "hogan.js": "~2.0.0",
    "mustache": "~0.8.1",
    "jade": "~1.1.5",
    "lodash": "~2.4.1",
    "js-beautify": "~1.4.2"
  },
  "devDependencies": {
    "grunt": "~0.4.0",
    "grunt-contrib-jshint": "~0.3.0",
    "grunt-contrib-nodeunit": "~0.1.2",
    "grunt-contrib-clean": "~0.4.0"
  },
  "peerDependencies": {
    "grunt": "~0.4.0"
  },
  "keywords": [
    "gruntplugin",
    "css",
    "dss",
    "styles",
    "docs",
    "documentation",
    "cli"
  ],
  "readmeFilename": "README.md",
  "directories": {
    "doc": "docs",
    "example": "example",
    "test": "test"
  }
}
